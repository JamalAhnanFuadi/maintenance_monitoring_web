<?xml version="1.0" encoding="utf-8"?>

    <!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>^/login$</from>
		<to>/login.jsp</to>
	</rule>

	<rule>
		<from>^/index$</from>
		<to>/WEB-INF/index.jsp</to>
	</rule>

	<rule>
		<from>^/user-management$</from>
		<to>/WEB-INF/user_management.jsp</to>
	</rule>

	<rule>
		<from>^/user-access-group$</from>
		<to>/WEB-INF/user-access-group.jsp</to>
	</rule>

	<rule>
		<from>^/application</from>
		<to>/WEB-INF/application.jsp</to>
	</rule>

</urlrewrite>